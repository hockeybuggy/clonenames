{
  "scripts": {
    "dev": "netlify dev",
    "start": "webpack-dev-server --hot",
    "production-build": "webpack --production",
    "test": "jest",
    "typecheck": "tsc -p .",
    "format": "prettier --write \"src/**/*.ts*\"",
    "check-format": "prettier --check \"src/**/*.ts*\"",
    "generate-bundle-analyzer-stats": "webpack --profile --json > raw_stats.json && tail -n+3 raw_stats.json > stats.json",
    "bundle-analyzer": "webpack-bundle-analyzer stats.json"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.10.4",
    "@hot-loader/react-dom": "^16.14.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.2",
    "@types/classnames": "^2.3.1",
    "@types/history": "^4.7.9",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/shortid": "^0.0.29",
    "@types/styled-components": "^5.1.12",
    "@types/uuid": "^8.3.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.11.1",
    "css-loader": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^6.3.1",
    "html-webpack-plugin": "^4.5.2",
    "jest": "^27.4.2",
    "netlify-cli": "^2.58.0",
    "prettier": "^2.3.2",
    "style-loader": "^2.0.0",
    "typescript": "^3.9.7",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "core-js": "^3.16.0",
    "faunadb": "^4.3.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^4.13.0",
    "react-is": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.1",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "^0.13.9",
    "shortid": "^2.2.16",
    "styled-components": "^5.3.0",
    "uuid": "^8.3.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./src/setupTests.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.ts"
    }
  }
}
