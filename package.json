{
  "scripts": {
    "dev": "netlify dev",
    "start": "webpack-dev-server --hot",
    "production-build": "webpack --production",
    "test": "jest",
    "typecheck": "tsc -p .",
    "format": "prettier --write \"src/**/*.ts*\"",
    "check-format": "prettier --check \"src/**/*.ts*\"",
    "generate-bundle-analyzer-stats": "webpack --profile --json > raw_stats.json && tail -n+3 raw_stats.json > stats.json",
    "bundle-analyzer": "webpack-bundle-analyzer stats.json"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@hot-loader/react-dom": "^16.10.2",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^11.2.2",
    "@types/classnames": "^2.2.9",
    "@types/history": "^4.7.7",
    "@types/jest": "^26.0.24",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/shortid": "^0.0.29",
    "@types/styled-components": "^5.1.2",
    "@types/uuid": "^8.0.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.11.1",
    "css-loader": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^5.0.13",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.2.2",
    "netlify-cli": "^2.58.0",
    "prettier": "^2.0.5",
    "style-loader": "^1.2.1",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "faunadb": "^2.14.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.21",
    "react-is": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "^0.13.7",
    "shortid": "^2.2.15",
    "styled-components": "^5.1.1",
    "uuid": "^8.3.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./src/setupTests.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.ts"
    }
  }
}
